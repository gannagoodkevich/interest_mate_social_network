<% if @commentable.class == Post %>
  $("#comments-post-<%= @commentable.id %>").append("<%= j render(@comment) %>")
  $("#comments-post-create").html("<%= j render(partial: 'form', locals: {comment: Comment.new, commentable: @commentable, my_url: user_post_comments_path(post_id: @commentable.id)}) %>")
<% else%>
  $("#comments-show-<%=@commentable.id %>").append("<%= j render(@comment) %>")
  $("#comments-show-create").html("<%= j render(partial: 'form', locals: {comment: Comment.new, commentable: @commentable, my_url: user_post_comment_comments_path(comment_id: @commentable.id)}) %>")
<% end%>