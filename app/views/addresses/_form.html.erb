<%= bootstrap_form_with(model: address, url: my_url, method: :put,  remote: true) do |f| %>
  <%= f.text_field :country %>
  <%= f.text_field :city %>
  <%= f.text_field :street %>
  <%= f.text_field :house_number %>
  <%= f.text_field :apartment_number %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>