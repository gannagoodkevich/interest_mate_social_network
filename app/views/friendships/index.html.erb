<p>
  <div class="row">
    <div class="col-sm-1">
    </div>
    <div class="col-sm-2">
      <%= render partial: "users/user_panel", locals: {user: @user} %>
    </div>
    <div class="col-sm-8">
      <div class="col-sm-2"></div>
      <div class="col-sm-10">

            <p></p>
            <div class="row">
              <div class="col-sm-6">

                <div class="col-sm-2"></div>
                <div class="col-sm-8">
                  <p></p>
                <%= link_to user_path(id: @friends.last.id), class: "card-link" do %>
                  <%= image_tag @friends.last.photo.image_url(:small), class: "medium-avatar-circle", data: { toggle: "tooltip", placement: "left"}, title: @friends.last.nickname %>
                <% end %>
                </div>
                <p></p>
                  <h4 class="text-muted"><%=  @friends.last.name%> <%=  @friends.last.surname %></h4>
              </div>
            </div>
      </div>
    </div>
  </div>
</p>

